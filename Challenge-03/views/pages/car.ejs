<!DOCTYPE html>
<html lang="en">

<head>
    <%- include("../partials/layouts/head"); %>
    <!-- CSS START -->
    <link rel="stylesheet" href="/stylesheets/pages/carsStyle.css" />
    <!-- CSS END -->
</head>

<body>
    <!-- SIDEBAR_ICON START -->
    <%- include("../partials/layouts/sidebarIcon"); %>
    <!-- SIDEBAR_ICON END -->
    <div class="box1">
        <header>
            <!-- NAVBAR START -->
            <%- include("../partials/layouts/navbar"); %>
            <!-- NAVBAR END -->
        </header>
        <main>
            <!-- SIDEBAR_MENU START -->
            <%- include("../partials/layouts/sidebarMenu"); %>
            <!-- SIDEBAR_MENU END -->
            <!-- CONTENT START -->
            <div class="content">
                <!-- UPPER START -->
                <div class="upper">
                    <span class="text1">Cars <i class='bx bx-chevron-right'></i> </span><span class="text2">List
                        car</span>
                </div>
                <!-- UPPER END -->
                <!-- MIDDLE START -->
                <div class="middle">
                    <span>List Car</span>
                    <a href="/addcars">
                        <button>+ Add New Car</button>
                    </a>
                </div>
                <!-- MIDDLE END -->
                <!-- LOWER START -->
                <div class="lower">
                    <div class="boxlower">
                        <button class="btn1">All</button>
                        <button class="btn2">Small</button>
                        <button class="btn3">Medium</button>
                        <button class="btn4">Large</button>
                    </div>
                </div>
                <!-- LOWER END -->
                <!-- MAIN START -->
                <div class="main">
                    <!-- CARDS START -->
                    <%- include("../partials/car/cards"); %>
                    <!-- CARDS END -->
                    <!-- MODAL START -->
                    <%- include("../partials/car/modal"); %>
                    <!-- MODAL END -->
                    <!-- TOAST START -->
                    <%- include("../partials/car/toast"); %>
                    <!-- TOAST END -->
                </div>
                <!-- MAIN END -->
            </div>
            <!-- CONTENT END -->
        </main>
    </div>
    <!-- JS START -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous">
    </script>
    <script src="/javascripts/carScript.js"></script>
    <!-- JS END  -->
</body>

</html>